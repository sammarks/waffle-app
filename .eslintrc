{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
   },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "prettier/prettier": ["error", {"singleQuote": true}]
  }
}
